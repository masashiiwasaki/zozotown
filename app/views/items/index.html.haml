    #headerInner.clearfix
      %ul#mainTab.clearfix
        %li#tabAll
          = link_to "すべて", "./?c=SwitchType&ts=0"
        %li#tabMEN
          = link_to "メンズ", "./?c=SwitchType&ts=1"
        %li#tabWOMEN
          = link_to "レディース", "./?c=SwitchType&ts=2"
        %li#tabKIDS
