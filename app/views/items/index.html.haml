      #rightContent.clearfix
        #rightContentMain
          %section#idxCouponItem.section
            .sectionHeader.clearfix.lined
              %h2 クーポン対象アイテム
            #couponItem.loader.clearfix.id10457
              %ul.rankingList.clearfix
                = render @items
            %p.more
              = link_to "クーポン対象アイテム一覧", "_search/?c=UnitShopSearch&cptypeid=10457"
