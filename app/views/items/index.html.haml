                %li
                  = link_to "ZOZOTOWNお買いものガイド", "http://zozo.jp/welcome/default.html"
                %li
                  = link_to "会員登録はこちらから", "https://zozo.jp/_member/regist/"
                %li.last
                  = link_to "メールマガジン購読","https://zozo.jp/_mail/"
              / /.txt
          / /#about
